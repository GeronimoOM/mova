{
  "name": "mova",
  "version": "1.0.0",
  "description": "Mova App",
  "author": "GeronimoOM",
  "license": "UNLICENSED",
  "devDependencies": {
    "husky": "8.0.0",
    "lerna": "7.4.2"
  },
  "scripts": {
    "start": "docker-compose -f docker-compose.dev.yml up",
