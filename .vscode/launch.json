{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Docker: Node.js API",
      "address": "localhost",
      "port": 9001,
      "cwd": "${workspaceFolder}",
      "localRoot": "${workspaceFolder}/api",
      "remoteRoot": "/app",
      "skipFiles": [
        "<node_internals>/**/*.js",
        "${workspaceFolder}/node_modules/**"
      ]
    }
  ]
}
